<script type="text/worker">

on("change:appraisal_ranks change:chemistry_ranks change:cultures_ranks change:history_ranks change:investigation_ranks change:medicine_ranks change:nature_ranks change:pantheons_ranks change:politics_ranks change:acting_ranks change:crafting_ranks change:dancing_ranks change:mimicry_ranks change:instruments_ranks change:publicspeaking_ranks change:singing_ranks change:storytelling_ranks change:trickery_ranks change:armor_ranks change:brawling_ranks change:conditioning_ranks change:initiative_ranks change:martialarts_ranks change:reflexes_ranks change:weaponry_ranks change:rageofberserker_ranks change:burstofcelerity_ranks change:perfectionoffocus_ranks change:bodyofiron_ranks change:alchemy_ranks change:arcana_ranks change:attunement_ranks change:cantrips_ranks change:casting_ranks change:mana_ranks change:ritual_ranks change:runes_ranks change:signaling_ranks change:wandage_ranks change:warding_ranks change:animalhandling_ranks change:horticulture_ranks change:navigation_ranks change:perception_ranks change:riding_ranks change:sailing_ranks change:survival_ranks change:tracking_ranks change:traps_ranks change:acrobatics_ranks change:athletics_ranks change:climbing_ranks change:contortions_ranks change:endurance_ranks change:gliding_ranks change:jumping_ranks change:swimming_ranks change:composure_ranks change:diplomacy_ranks change:expression_ranks change:insight_ranks change:intimidation_ranks change:persuasion_ranks change:trading_ranks change:ambush_ranks change:deception_ranks change:disarm_ranks change:disguise_ranks change:hiding_ranks change:larceny_ranks change:sleightofhand_ranks change:sniping_ranks change:stalking_ranks streetwise_ranks", function(eventInfo) {
    var target = /.*_/.exec(eventInfo.sourceAttribute) + "bonus";
    var ranks = Number(eventInfo.newValue);
    var attrs = {};
    
    if (ranks === NaN){
        attrs[target] = 0;
    }else{
        var bonus = Math.min(ranks,10) * 5 + Math.max(Math.min(ranks-10, 10),0) * 2 + Math.max(ranks-20, 0);
        attrs[target] = bonus;
    }
    
    setAttrs(attrs);
});

</script>
